(define cont #f)
(define out (current-output-port))
(define (bool->string b) (if b "TRUE" "FALSE"))
(define (compose a b) (lambda (x) (a (b x))))
(define (my-show str) (write-string str out))
(define (bar y) (set! cont y))
(define (my-not b) (if b #f #t))
(define (foo x) (call/cc bar) (set! x (my-not x)) ((compose my-show bool->string) x))
(foo #t)
(cont #t)